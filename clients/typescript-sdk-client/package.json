{
  "name": "simple-oauth-mcp-client",
  "version": "1.12.1",
  "description": "SimpleOauthClient copied from the typescript MCP SDK, with some minimal changes reqquired for the demo",
  "license": "MIT",
  "author": "Anthropic, PBC (https://anthropic.com)",
  "homepage": "https://modelcontextprotocol.io",
  "bugs": "https://github.com/modelcontextprotocol/typescript-sdk/issues",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modelcontextprotocol/typescript-sdk.git"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "modelcontextprotocol",
    "mcp"
  ],
  "exports": {
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/esm/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "MCP_SERVER_URL='https://mcp.demo.example' NODE_EXTRA_CA_CERTS=~/Desktop/example.ca.crt tsx simpleOAuthClient.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.0",
    "undici": "^7.10.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "tsx": "^4.20.5"
  },
  "resolutions": {
    "strip-ansi": "6.0.1"
  }
}
